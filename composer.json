{
    "name": "creative-syntax/page-loader",
    "description": "A laravel package for auto page loading system",
    "type": "laravel-package",
    "license": "MIT",
    "keywords": [
        "page-loader",
        "laravel-page-loader",
        "creative-syntax",
        "onexcrm",
        "arindam"
    ],
    "authors": [
        {
            "name": "Arindam Roy",
            "email": "arindam.roy.developer@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0"
    },
    "autoload": {
        "psr-4": {
            "CreativeSyntax\\PageLoader\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CreativeSyntax\\PageLoader\\CSPageLoader"
            ]
        }
    }
}
